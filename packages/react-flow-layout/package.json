{
  "name": "react-flow-layout",
  "version": "1.5.0",
  "description": "react-flow-layout",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "repository": "https://github.com/naguvan/react-flow-layout.git",
  "author": "naguvan <naguvan@outlook.com>",
  "license": "MIT",
  "private": false,
  "bugs": {
    "url": "https://github.com/naguvan/react-flow-layout/issues"
  },
  "files": [
    "lib/",
    "lib-esm/"
  ],
  "peerDependencies": {
    "@material-ui/core": "^3.1.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "dependencies": {
    "@material-ui/core": "3.2.0",
    "classnames": "2.2.6",
    "react": "16.5.2",
    "react-dom": "16.5.2"
  },
  "devDependencies": {
    "@types/classnames": "2.2.6",
    "@types/jest": "23.3.5",
    "@types/node": "10.11.6",
    "@types/react": "16.4.16",
    "@types/react-dom": "16.0.9",
    "cross-env": "5.2.0",
    "husky": "1.1.1",
    "jest": "23.6.0",
    "lerna": "3.4.3",
    "prettier": "1.14.3",
    "pretty-quick": "1.7.0",
    "rimraf": "2.6.2",
    "ts-jest": "23.10.4",
    "ts-loader": "5.2.1",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.15.0",
    "tslint-loader": "3.5.4",
    "tslint-react": "3.6.0",
    "typescript": "3.1.2"
  },
  "keywords": [
    "react",
    "flow",
    "layout",
    "typescript"
  ],
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "roots": [
      "<rootDir>/src/"
    ]
  },
  "scripts": {
    "prepare": "npm run build",
    "prebuild": "npm run lint && npm run clean",
    "clean": "rimraf {lib,lib-esm}",
    "lint": "tslint -p .",
    "precommit": "npm run lint && pretty-quick --staged",
    "pretty": "prettier --write 'src/**/*.{ts,tsx}'",
    "test": "cross-env NODE_ENV=production TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\",\\\"declaration\\\":false} jest --runInBand",
    "env": "cross-env NODE_ENV=production TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\"}",
    "dev:tsc": "tsc && tsc -m es6 --outDir lib-esm",
    "build": "npm run dev:tsc"
  }
}
